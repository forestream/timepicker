{
	"compilerOptions": {
		"target": "es2016",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"jsx": "preserve",
		"plugins": [{ "name": "typescript-plugin-css-modules" }]
	},
	"$schema": "https://json.schemastore.org/tsconfig",
	"include": ["src/**/*"]
}
